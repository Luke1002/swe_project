<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<HBox xmlns="http://javafx.com/javafx/17.0.6" xmlns:fx="http://javafx.com/fxml/1">
<children>
<VBox HBox.hgrow="ALWAYS">
    <children>
        <Label fx:id="publisherLabel" text="Editore">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label fx:id="frequencyLabel" text="Frequenza">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
    </children>
    <HBox.margin>
        <Insets />
    </HBox.margin>
</VBox>
<VBox HBox.hgrow="SOMETIMES">
    <children>
        <Label fx:id="monthLabel" text="Mese">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
        <Label fx:id="dayLabel" text="Giorno">
            <font>
                <Font size="16.0" />
            </font>
        </Label>
    </children>
    <HBox.margin>
        <Insets />
    </HBox.margin>
</VBox>
</children>
<padding>
    <Insets left="10.0" right="10.0" />
</padding>
</HBox>
